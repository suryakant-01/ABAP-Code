REPORT ZSUBR_EXIT_01.

DATA : N1 TYPE I,
       N2 TYPE I,
       R1 TYPE I.

N1 = 20.
N2 = 10.

PERFORM OPERATION USING N1 N2 CHANGING R1.
WRITE: / 'RESULT OF', N1, 'AND', N2, 'IS', R1.
ULINE.

N1 = 10.
N2 = 10.
PERFORM OPERATION USING N1 N2 CHANGING R1.
WRITE: / 'RESULT OF', N1, 'AND', N2, 'IS', R1.
ULINE.

FORM OPERATION USING F1 F2 CHANGING RES.

  RES = N1 + N2.
  WRITE: / 'HELLO WORLD'.
  ENDFORM.
